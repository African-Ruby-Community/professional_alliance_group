<head>
	<meta charset="utf-8"/>
	<title>{{ page.title | default: "Admanus Group - Expertise Connected" }}</title>
	<meta name="description" content="{{ page.description | default: "Bridging individual skills and knowledge to create a dynamic and effective collaborative network." }}"/>

	<!-- Favicon -->
	<link rel="icon" href="{{ '/images/favicon.ico' | prepend: site.baseurl }}" type="image/x-icon"/>
	<link rel="icon" type="image/png" sizes="16x16" href="{{ '/images/favicons/favicon-16x16.png' | prepend: site.baseurl }}"/>
	<link rel="icon" type="image/png" sizes="32x32" href="{{ '/images/favicons/favicon-32x32.png' | prepend: site.baseurl }}"/>
	<link rel="apple-touch-icon" sizes="180x180" href="{{ '/images/favicons/apple-touch-icon.png' | prepend: site.baseurl }}"/>
	<link rel="manifest" href="{{ '/site.webmanifest' | prepend: site.baseurl }}"/>
	<meta name="apple-mobile-web-app-title" content="Admanus Group"/>
	<meta name="apple-mobile-web-app-status-bar-style" content="black-translucent"/>

	<!-- For Android devices -->
	<meta name="theme-color" content="#ffffff"/>
	<meta name="mobile-web-app-capable" content="yes"/>
	<meta name="application-name" content="Admanus Group"/>
	<link rel="icon" type="image/png" sizes="192x192" href="{{ '/images/favicons/android-chrome-192x192.png' | prepend: site.baseurl }}"/>
	<link rel="icon" type="image/png" sizes="512x512" href="{{ '/images/favicons/android-chrome-512x512.png' | prepend: site.baseurl }}"/>

	<!-- CSS Style -->
	<link href="{{ site.baseurl }}/assets/css/bootstrap.css" rel="stylesheet" />
	<link href="{{ site.baseurl }}/assets/css/bootstrapV5.css" rel="stylesheet" />
	<link href="{{ site.baseurl }}/assets/css/style.css" rel="stylesheet" />
	<link href="{{ site.baseurl }}/assets/css/responsive.css" rel="stylesheet" />
	<link href="{{ site.baseurl }}/assets/css/color-switcher-design.css" rel="stylesheet" />

	<link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.0.0-beta3/css/all.min.css">


	{% include google_analytics.liquid %}

	<!-- Meta Tags for Responsive Design -->
	<meta http-equiv="X-UA-Compatible" content="IE=edge"/>
	<meta name="viewport" content="width=device-width, initial-scale=1.0"/>

	<!-- Open Graph meta tags -->
	<meta property="og:title" content="{{ page.title | default: 'Admanus Group' }}"/>
	<meta property="og:description" content="{{ page.description | default: 'Bridging individual skills and knowledge to create a dynamic and effective collaborative network.' }}"/>
	<meta property="og:image" content="https://admanusgroup.com/images/agroup.png"/>
	<meta property="og:url" content="{{ page.url | absolute_url | default: site.url }}"/>
	<meta property="og:type" content="website"/>
	<meta property="og:site_name" content="Admanus Group"/>

	<!-- Twitter meta tags -->
	<meta name="twitter:card" content="summary_large_image"/>
	<meta property="twitter:domain" content="admanusgroup.com">
	<meta property="twitter:url" content="https://admanusgroup.com">
	<meta name="twitter:title" content="{{ page.title | default: 'Admanus Group: Connect, Learn, & Grow with Ruby' }}"/>
	<meta name="twitter:description" content="{{ page.description | default: 'Bridging individual skills and knowledge to create a dynamic and effective collaborative network.' }}"/>
	<meta name="twitter:image" content="https://admanusgroup.com/images/agroup.png"/>


	{% comment %}HTLM to PDF CDN{% endcomment %}
	<script src="https://cdnjs.cloudflare.com/ajax/libs/html2pdf.js/0.9.2/html2pdf.bundle.min.js"></script>
	<script src="https://cdnjs.cloudflare.com/ajax/libs/html2canvas/1.4.1/html2canvas.min.js"></script>

</head>
