<!-- Main Header-->
<header class="main-header header-style-two">
    <div class="main-box">
        <div class="auto-container clearfix">
            <div class="logo-box">
                <div class="logo">
                    <a href="/"
                    ><img
                                class="theme-sensitive-logo"
                                src="{{ '/images/logo/ag_logo_coloured.png' | prepend: site.baseurl }}"
                                data-light-src="{{ '/images/logo/ag_logo_coloured.png' | prepend: site.baseurl }}"
                                data-dark-src="{{ '/images/logo/aglogo.png' | prepend: site.baseurl }}"
                                alt="Admanus Group"
                                title=""
                                width=""
                                height=""
                                loading="lazy"
                        /></a>
                </div>
            </div>

            <!--Nav Box-->
            <div class="nav-outer clearfix">
                <!--Mobile Navigation Toggler-->
                <div class="mobile-nav-toggler">
                    <span class="icon flaticon-menu"></span>
                </div>
                <!-- Main Menu -->
                <nav class="main-menu navbar-expand-md navbar-light">
                    <div class="navbar-header">
                        <!-- Toggle Button -->
                        <button
                                class="navbar-toggler"
                                type="button"
                                data-toggle="collapse"
                                data-target="#navbarSupportedContent"
                                aria-controls="navbarSupportedContent"
                                aria-expanded="false"
                                aria-label="Toggle navigation"
                        >
                            <span class="icon flaticon-menu-button"></span>
                        </button>
                    </div>

                    <div
                            class="collapse navbar-collapse clearfix"
                            id="navbarSupportedContent"
                    >
                        <ul class="navigation clearfix">
                            <li>
                                <a href="{{ '/' | relative_url }}">Home</a>
                            </li>

                            <li {% if page.url == '/about-us' %} class="current"{% endif %}>
                                <a href="{{ '/about-us' | relative_url }}">About</a>
                            </li>

                            <li {% if page.url == '/members' %} class="current"{% endif %}>
                                <a href="{{ '/members' | relative_url }}">Members</a>
                            </li>

                            <li {% if page.url == '/collaborations' %} class="current"{% endif %}>
                                <a href="{{ '/collaborations' | relative_url }}">Collaborations</a>
                            </li>
                        </ul>

                    </div>
                </nav>
                <!-- Main Menu End-->

            </div>
        </div>
    </div>

    <!-- Mobile Menu  -->
    <div class="mobile-menu">
        <div class="menu-backdrop"></div>
        <div class="close-btn">
            <span class="icon flaticon-cancel-1"></span>
        </div>

        <!--Here Menu Will Come Automatically Via Javascript / Same Menu as in Header-->
        <nav class="menu-box">
            <div class="nav-logo">
                <a href="index.html"
                ><img
                            class="theme-sensitive-logo"
                            src="{{ '/images/logo/ag_logo_coloured.png' | prepend: site.baseurl }}"
                            data-light-src="{{ '/images/logo/ag_logo_coloured.png' | prepend: site.baseurl }}"
                            data-dark-src="{{ '/images/logo/aglogo.png' | prepend: site.baseurl }}"
                            alt="Admanus Group Logo"
                            title=""></a
                >
            </div>

            <ul class="navigation clearfix">
                <!--Keep This Empty / Menu will come through Javascript-->
            </ul>
        </nav>
    </div>
    <!-- End Mobile Menu -->
</header>
